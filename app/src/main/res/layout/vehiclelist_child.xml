<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/_95sdp"
    android:background="@color/white"
    android:orientation="vertical"
    android:padding="@dimen/_10sdp">

    <com.chauthai.swipereveallayout.SwipeRevealLayout
        android:id="@+id/swipeRefresh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:dragEdge="right"
        app:mode="same_level">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:background="@color/colorPrimary">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical|center"
                android:orientation="horizontal">

                <com.poshwash.android.customViews.CustomTexViewRegular
                    android:id="@+id/editTv"
                    android:layout_width="@dimen/_60sdp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center"
                    android:background="@android:color/darker_gray"
                    android:gravity="center"
                    android:text="@string/edit"
                    android:textColor="@color/whitecolor"
                    android:textSize="@dimen/_12sdp" />

                <com.poshwash.android.customViews.CustomTexViewRegular
                    android:id="@+id/deleteTv"
                    android:layout_width="@dimen/_60sdp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical|center"
                    android:layout_toEndOf="@+id/editTv"
                    android:background="@android:color/holo_red_dark"
                    android:gravity="center"
                    android:text="@string/delete"
                    android:textColor="@color/whitecolor"
                    android:textSize="@dimen/_12sdp" />
            </RelativeLayout>

        </FrameLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/_75sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp">

            <ImageView
                android:id="@+id/img_car"
                android:layout_width="@dimen/_110sdp"
                android:layout_height="@dimen/_75sdp"
                android:layout_centerVertical="true"
                android:background="@drawable/recentgleimageview"
                android:scaleType="centerCrop"
                android:src="@drawable/no_image" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_10sdp"
                android:layout_toEndOf="@+id/img_car"
                android:orientation="vertical">

                <com.poshwash.android.customViews.CustomTexViewBold
                    android:id="@+id/txt_carname"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:gravity="start"
                    android:singleLine="true"
                    android:text="@string/tour_car"
                    android:textColor="@color/signup_btn"
                    android:textSize="@dimen/_15sdp"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/model_number"
                        android:textColor="@color/blackcolor"
                        android:textSize="@dimen/_11sdp" />


                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:id="@+id/txt_modalnumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:maxLines="1"
                        android:text="BASDIDSF"
                        android:textColor="@color/darkgraycolor"
                        android:textSize="@dimen/_11sdp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:visibility="visible"
                    android:weightSum="2">

                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/color"
                        android:textColor="@color/blackcolor"
                        android:textSize="@dimen/_11sdp" />

                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:id="@+id/txt_colornmame"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="red"
                        android:textColor="@color/darkgraycolor"
                        android:textSize="@dimen/_11sdp" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2sdp"
                    android:weightSum="2">

                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:maxLines="1"
                        android:singleLine="true"
                        android:text="@string/plate_number"
                        android:textColor="@color/blackcolor"
                        android:textSize="@dimen/_11sdp" />

                    <com.poshwash.android.customViews.CustomTexViewRegular
                        android:id="@+id/txt_plate_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:ellipsize="end"
                        android:gravity="start"
                        android:maxLines="1"
                        android:text="123456799"
                        android:textAllCaps="true"
                        android:textColor="@color/darkgraycolor"
                        android:textSize="@dimen/_11sdp" />

                </LinearLayout>

            </LinearLayout>


        </RelativeLayout>
    </com.chauthai.swipereveallayout.SwipeRevealLayout>
</LinearLayout>